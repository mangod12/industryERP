{
    "name": "kumarbrothers/steel-erp",
    "type": "project",
    "description": "Kumar Brothers Steel Fabrication ERP - Complete manufacturing, inventory, and scrap management system for steel industry.",
    "keywords": [
        "laravel",
        "steel-fabrication",
        "erp",
        "manufacturing",
        "inventory",
        "scrap-management"
    ],
    "authors": [
        {
            "name": "Kumar Brothers Steel"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^v3.0.0",
        "directorytree/ldaprecord-laravel": "^v3.3.5",
        "doctrine/dbal": "^3.8",
        "dragonbe/vies": "^2.3",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "horstoeko/zugferd": "^1",
        "jenssegers/agent": "^2.6",
        "jeroennoten/laravel-adminlte": "^3.9.2",
        "laravel/framework": "12.1.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^v4.5.2",
        "laravolt/avatar": "^6.2",
        "livewire/livewire": "^3.4",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^2.0",
        "milon/barcode": "^12.0",
        "monolog/monolog": "^3.0",
        "phpoffice/phpspreadsheet": "^1.29.1",
        "predis/predis": "^2.2",
        "protonemedia/laravel-cross-eloquent-search": "^3.2.0",
        "psr/simple-cache": "2.0",
        "pusher/pusher-php-server": "^7.2",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-permission": "^6.9",
        "webklex/laravel-pdfmerger": "^1.3"
    },
    "require-dev": {
        "dominikb/composer-license-checker": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^2.0",
        "laravel/sail": "^1.12",
        "laravel/telescope": "*",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "8.8",
        "phpunit/phpunit": "11.5.50"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
